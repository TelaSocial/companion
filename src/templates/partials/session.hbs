     <a href="#session-info-{{this.id}}" class="session-title">
        {{this.title}}
    </a>

<div id="session-info-{{this.id}}" class="content">
    <p class="session-abstract">
        {{this.abstract}}
    </p>
    {{#if this.authors}}
        <h4>Autores</h4>
        {{#each this.authors}}
            <span class="secondary radius label">{{this.name}}</span>
        {{/each}}
    {{/if}}
    <p>
        <a
            class="calendar-add-button"
            href="#"
        >
            Add to calendar
        </a>
    </p>
    <span class="secondary radius label">{{this.roomName}}</span>
